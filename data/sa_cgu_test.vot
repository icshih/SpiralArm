<INFO name="QUERY"><![CDATA[SELECT TOP 10 g.source_id, g.l, g.b, g.ra, g.ra_error, g.dec, g.dec_error, g.pmra, g.pmra_error, g.pmdec, g.pmdec_error, g.parallax, g.parallax_error, g.phot_g_mean_mag, u.original_ext_source_id AS ucac4_id 
FROM gaiadr1.ucac4_best_neighbour AS u, (SELECT source_id, l, b, ra, ra_error, dec, dec_error, pmra, pmra_error, pmdec, pmdec_error, parallax, parallax_error, phot_g_mean_mag FROM gaiadr1.gaia_source WHERE pmra IS NOT NULL AND pmdec IS NOT NULL AND parallax IS NOT NULL AND (l < 17 OR l > 285 OR (l > 72 AND l < 222))) AS g 
WHERE g.source_id = u.source_id AND u.number_of_mates = 0]]></INFO>
